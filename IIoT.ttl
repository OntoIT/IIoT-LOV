                                                           owl:onProperty <https://ontoit.github.io/IIoT/Device/isMeasuredBy> ;
                                                           owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onClass <https://w3id.org/IIoT/#RTD_sensor>
